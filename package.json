{
  "name": "js-webinar",
  "version": "1.0.0",
  "description": "Tasks for JS webinar",
  "scripts": {
    "test:module-1": "mocha module-1/test/**/*.spec.js",
    "test:module-2": "mocha module-2/test/**/*.spec.js",
    "test:module-3": "npx playwright test --headed",
    "test:module-4": "mocha module-4/test/**/*.spec.js",
    "test:module-5": "cucumber-js --require module-5/config/cucumber.conf.js module-5/features/*.feature --require module-5/step_definitions/*.js -f @cucumber/pretty-formatter",
    "test:plus": "mocha plus/test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szikszail/js-webinar.git"
  },
  "author": "Laszlo Szikszai <Laszlo_Szikszai@epam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szikszail/js-webinar/issues"
  },
  "homepage": "https://github.com/szikszail/js-webinar#readme",
  "dependencies": {
    "@cucumber/cucumber": "^8.2.2",
    "chai": "^4.2.0",
    "mocha": "^9.2.1",
    "playwright": "^1.19.2",
    "sinon": "^13.0.1"
  },
  "devDependencies": {
    "@cucumber/pretty-formatter": "^1.0.0-alpha.2",
    "@playwright/test": "^1.19.2"
  }
}
